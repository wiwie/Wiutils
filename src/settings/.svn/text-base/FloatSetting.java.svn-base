package utils.settings;

import java.text.ParseException;

public class FloatSetting extends NumberSetting<Float> {

	public FloatSetting(final String name_) {
		super(name_, null);
	}

	public FloatSetting(final String name_, final Float initValue) {
		super(name_, initValue);
	}

	@Override
	public void parseValueFromString(final String rawString)
			throws ParseException {
		try {
			this.val_ = Float.valueOf(rawString);
		} catch (final NumberFormatException e) {
			throw new ParseException(e.getMessage(), 0);
		}
	}

}